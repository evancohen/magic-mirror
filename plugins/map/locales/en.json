{
	"commands":{
		"map_show": {
            "text": "Show map",
            "voice": "Show map",
            "description": "Shows a map of your current city."
        },
        "map_location": {
            "text": "Show map of ________",
            "voice": "Show (me a) map of *location",
            "description": "Shows a map of the given location."
        },
        "map_zoom_in": {
            "text": "Zoom in",
            "voice": "(map) zoom in",
            "description": "Zooms in the current map."
        },
        "map_zoom_out": {
            "text": "Zoom out",
            "voice": "(map) zoom out",
            "description": "Zooms out the current map."
        },
        "map_zoom_point": {
            "text": "Zoom to ________",
            "voice": "(map) zoom (to) *value",
            "description": "Zooms to indicated point."
        },
        "map_zoom_reset": {
            "text": "Reset zoom",
            "voice": "(map) reset zoom",
            "description": "Resets zoom."
        }
	}
}